ult$aggregate==1 &test_label==0\n      fp = length(test[fp_row,1])\n      total_fp = total_fp + fp;\n      # of FN\n      fn_row=result$aggregate==0 & test_label==1\n      fn = length(test[fn_row,1])\n      total_fn = total_fn + fn;\n    }\n    probs_result_final<- rbind(probs_result[probs_result[,3]==1, c(2,3)], probs_result[probs_result[,3]==0, c(1,3)])\n    \n    idx <-  order(probs_result_final[,1], decreasing = TRUE)\n    probs_result_final <- probs_result_final[idx,]\n    file_write_name = paste(file_dir, date,\" \", type, option, test_opt, sep=\"\");\n    write.table(x=probs_result_final, file=paste(file_write_name, \"probability.csv\", sep=\"_\"), sep=\",\", col.names = FALSE, row.names = FALSE, append = FALSE)\n    \n  }\n  \n}\nwriteLines(\"========================================\", con=fileConn)\nclose(fileConn)\n\n",
